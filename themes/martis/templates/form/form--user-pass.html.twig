{% extends "form--user-form.html.twig" %}
{% block form_header %}
  {{ 'Reset your password'|t }}
{% endblock %}
{% block form_footer %}
  <h4 class="form-title">{{('Back to:'|t)}}</h4>
  <a class="button" href="{{ path('user.login') }}">{{ 'Login'|t }}</a>
  <a class="button"  href="{{ path('user.register') }}">{{ 'Register'|t }}</a>
{% endblock %}